[
  {
    "layout": "vertical",
    "components": [
      {
        "innerId": "ComponentPackage",
        "component": "org.abx.app.components.leftpanel.ComponentPackage",
        "size": "l2",
        "env": ""
      },
      {
        "innerId": "ProcessCloneComponent",
        "component": "org.abx.app.components.clone.ProcessCloneComponent",
        "size": "l2",
        "env": ""
      },
      {
        "innerId": "RenameComponentPopup",
        "component": "org.abx.app.components.rename.RenameComponentPopup",
        "size": "l12",
        "env": ""
      },
      {
        "innerId": "CloneComponentPopup",
        "component": "org.abx.app.components.clone.CloneComponentPopup",
        "size": "l2",
        "env": ""
      },
      {
        "innerId": "ProcessRenameComponent",
        "component": "org.abx.app.components.rename.ProcessRenameComponent",
        "size": "l12",
        "env": ""
      },
      {
        "innerId": "elem5",
        "component": "org.abx.app.components.leftpanel.DeleteComponentButton",
        "size": "l12",
        "env": ""
      },
      {
        "innerId": "DeleteComponentConfirm",
        "component": "org.abx.app.components.leftpanel.DeleteComponentConfirm",
        "size": "l12",
        "env": ""
      }
    ],
    "name": "org.abx.app.components.leftpanel.MenuContainer",
    "isContainer": true,
    "js": [
      {
        "env": "",
        "value": "$(ComponentPackage).on('change', ()=>{selectPackage( $(ComponentPackage).val());});"
      },
      {
        "env": "",
        "value": "workingEnv.ComponentPackage=ComponentPackage;"
      },
      {
        "env": "",
        "value": "workingEnv.CloneComponentPopup=CloneComponentPopup;"
      },
      {
        "env": "",
        "value": "$(ProcessCloneComponent).click(  ()=>  M.Modal.init(CloneComponentPopup).open() );"
      },
      {
        "env": "",
        "value": "$(ProcessRenameComponent).click(  ()=>  M.Modal.init(RenameComponentPopup).open() );"
      },
      {
        "env": "",
        "value": "$(ProcessRenameComponent).click(()=>  RenameComponentPopup.setName($(workingEnv.ComponentName).val()) );"
      },
      {
        "env": "",
        "value": "$(ProcessCloneComponent).click(()=>  CloneComponentPopup.setName($(workingEnv.ComponentName).val()+\"_2\") );"
      }
    ]
  },
  {
    "specs": [{
      "env": "",
      "value": {
        "cancel": "Cancel",
        "ok": "Delete",
        "title": "Confirm Delete",
        "content": "Do you want to delete element?"
      }
    }],
    "name": "org.abx.app.components.leftpanel.DeleteComponentConfirm",
    "isContainer": false,
    "js": [{
      "env": "",
      "value": "$(self).click(()=>{\n   $.get(`/components/delete/${workingEnv.component.name}`,\n   ()=>{location.reload();}) \n});"
    }],
    "type": "okCancelModal"
  },
  {
    "specs": [{
      "env": "",
      "value": {
        "src": "/components/packages",
        "title": "Component Package"
      }
    }],
    "name": "org.abx.app.components.leftpanel.ComponentPackage",
    "isContainer": false,
    "js": [],
    "type": "select"
  },
  {
    "specs": [{
      "env": "",
      "value": {"title": "Delete Component"}
    }],
    "name": "org.abx.app.components.leftpanel.DeleteComponentButton",
    "isContainer": false,
    "js": [{
      "env": "",
      "value": "$(self).click(()=>{\n   M.Modal.init(DeleteComponentConfirm).open();\n});"
    }],
    "type": "button"
  }
]