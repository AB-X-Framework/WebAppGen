      {
        "env": "",
        "value": "$(LeftPanel.MapsPackage).change(()=>{\n    $(LeftPanel.MapName).empty();\n    const packageName = $(LeftPanel.MapsPackage).val();\n    \n    $.get(`/resources/maps/packages/${packageName}`,(packages)=>{\n        packages.forEach((packageName)=>{\n            addAutocompleteValue(LeftPanel.MapName,packages)\n        });\n         $(LeftPanel.MapName).val(\"\");\n    });\n});"
      },
      {
        "env": "",
        "value": "workingEnv.currPage = 0;\nworkingEnv.MapBody = self;\nworkingEnv.updatedMap = {};\n\nworkingEnv.renderNextValues = ()=>{\n    App.process(\"webappgen.mapValues\",{\n        \"map\":$(LeftPanel.MapName).val(),\n        \"page\":workingEnv.currPage,\n        \"size\":10\n    },(componentSpecs)=>{\n        workingEnv.updatedMap = {};\n        componentSpecs.js = \"\";\n        const output = [];\n        PageContent.renderComponent(output, js, componentSpecs);\n        $(RightPanel.DetailsPanel.DetailsContainer).html(output.join(\"\"));\n        M.updateTextFields();\n    })\n};\n\n$(LeftPanel.MapName).change(()=> {\n    workingEnv.currPage = 0;\n});"
      }
    ]
