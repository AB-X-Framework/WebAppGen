    "name": "webappgen.arraypair.ArrayPairBody",
    "isContainer": true,
    "js": [
      {
        "env": "",
        "value": "$(LeftPanel.ArrayPairsPackage).change(()=>{\n    clearAutocomplete(LeftPanel.ArrayPairName);\n    \n    const packageName = $(LeftPanel.ArrayPairsPackage).val();\n    if (packageName !== \"\"){\n        $.get(`/resources/packages/arrayPairs/${packageName}`,(arraysPerPackage)=>{\n            arraysPerPackage.forEach((arrayPairName)=>{\n                addAutocompleteValue(LeftPanel.ArrayPairName,arrayPairName)\n            });\n             $(LeftPanel.ArrayPairName).val(\"\");\n        });\n    }\n});"
      },
      {
        "env": "",
        "value": "$(LeftPanel.MenuPanel.Owner).on('input',()=>{\n   workingEnv.meta.owner =  $(LeftPanel.MenuPanel.Owner).val();\n   workingEnv.SaveArrayPair.markChanged();\n});\n\n$(LeftPanel.MenuPanel.Access).change(()=>{\n   workingEnv.meta.access =  $(LeftPanel.MenuPanel.Access).val();\n   workingEnv.SaveArrayPair.markChanged();\n});"
      },
      {
        "env": "",
        "value": "workingEnv.currPage = 0;\nworkingEnv.ArrayPairBody = self;\nworkingEnv.updatedArrayPair = {};\nworkingEnv.entriesPerPage = 10;\nworkingEnv.entryCount=0;\nworkingEnv.renderNextValues = ()=>{\n    if (workingEnv.arrayPairName === \"\"){\n        return;\n    }\n    App.process(\"webappgen.arrayPairValues\",{\n        \"arrayPair\":workingEnv.arrayPairName,\n        \"page\":workingEnv.currPage,\n        \"size\":workingEnv.entriesPerPage\n    },(componentSpecs)=>{\n        workingEnv.SaveArrayPair.markSaved();\n        const init = (workingEnv.currPage*workingEnv.entriesPerPage)+1;\n        let end = ((workingEnv.currPage+1)*workingEnv.entriesPerPage);\n        end = Math.min(workingEnv.entryCount,end);\n        $(LeftPanel.MenuPanel.Label).text(`Entries ${init} - ${end} of ${workingEnv.entryCount}`);\n        workingEnv.updatedArrayPair = {};\n        componentSpecs.js = \"\";\n        const output = [];\n        const js =[];\n        PageContent.renderComponent(output, js, componentSpecs);\n        $(RightPanel.DetailsPanel.DetailsContainer).html(output.join(\"\"));\n        M.updateTextFields();\n        PageContent.processJS(js);\n    });\n};\nworkingEnv.renderArrayPair = ()=> {\n    workingEnv.arrayPairName = $(LeftPanel.ArrayPairName).val();\n    if (workingEnv.arrayPairName !== \"\"){\n        $.get(`/resources/arrayPairs/${workingEnv.arrayPairName}`,(arrayPairData)=>{\n            workingEnv.entryCount = arrayPairData.count;\n            workingEnv.meta = arrayPairData;\n            $(LeftPanel.MenuPanel.Owner).val(arrayPairData.owner);\n            $(LeftPanel.MenuPanel.Access).val(arrayPairData.access).formSelect();\n        });\n        workingEnv.currPage = 0;\n        workingEnv.renderNextValues();\n    }\n}\n$(LeftPanel.ArrayPairName).change(workingEnv.renderArrayPair);"
      }
    ]
  },
